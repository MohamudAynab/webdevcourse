{
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "settings": {
    "terminal.integrated.defaultProfile.linux": "/bin/bash"
  },
  "extensions": [
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode",
    "ms-azuretools.vscode-docker"
  ],
  "postCreateCommand": "",
  "forwardPorts": [5500],
  "portsAttributes": {
    "5500": {
      "label": "Live Server",
      "onAutoForward": "openBrowser"
    }
  },
  "remoteUser": "vscode"
}
